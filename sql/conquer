/*!50003 DROP PROCEDURE IF EXISTS `ChangePlayerName` */;
/*!50003 SET @saved_cs_client      = @@character_set_client */ ;
/*!50003 SET @saved_cs_results     = @@character_set_results */ ;
/*!50003 SET @saved_col_connection = @@collation_connection */ ;
/*!50003 SET character_set_client  = utf8mb4 */ ;
/*!50003 SET character_set_results = utf8mb4 */ ;
/*!50003 SET collation_connection  = utf8mb4_general_ci */ ;
/*!50003 SET @saved_sql_mode       = @@sql_mode */ ;
/*!50003 SET sql_mode              = 'STRICT_TRANS_TABLES,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION' */ ;
DELIMITER ;;
CREATE DEFINER=`root`@`localhost` PROCEDURE `ChangePlayerName`(IN idTarget INT, IN newName VARCHAR(16))
BEGIN
	#Routine body goes here...
	DECLARE rowMatch INT;
	SET rowMatch = (SELECT COUNT(*) FROM cq_user WHERE id=idTarget);
	
	IF rowMatch > 0
	THEN
		UPDATE `cq_user` SET `name`=newName WHERE `id`=idTarget LIMIT 1;
		UPDATE `cq_enemy` SET enemyname=newName WHERE enemy=idTarget;
		UPDATE cq_friend SET friendname=newName WHERE friend=idTarget;
		UPDATE cq_peerage SET user_name=newName WHERE user_id=idTarget LIMIT 1;
		UPDATE cq_syndicate SET leader_name=newName WHERE leader_id=idTarget AND del_flag IS NULL;
	END IF;
END ;;
DELIMITER ;
/*!50003 SET sql_mode              = @saved_sql_mode */ ;
/*!50003 SET character_set_client  = @saved_cs_client */ ;
/*!50003 SET character_set_results = @saved_cs_results */ ;
/*!50003 SET collation_connection  = @saved_col_connection */ ;
/*!50003 DROP PROCEDURE IF EXISTS `DistributeVipRewards` */;
/*!50003 SET @saved_cs_client      = @@character_set_client */ ;
/*!50003 SET @saved_cs_results     = @@character_set_results */ ;
/*!50003 SET @saved_col_connection = @@collation_connection */ ;
/*!50003 SET character_set_client  = utf8mb4 */ ;
/*!50003 SET character_set_results = utf8mb4 */ ;
/*!50003 SET collation_connection  = utf8mb4_general_ci */ ;
/*!50003 SET @saved_sql_mode       = @@sql_mode */ ;
/*!50003 SET sql_mode              = 'STRICT_TRANS_TABLES,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION' */ ;
DELIMITER ;;
CREATE DEFINER=`root`@`localhost` PROCEDURE `DistributeVipRewards`()
BEGIN
	#Routine body goes here...
	DECLARE usr_id INT DEFAULT 0;
	DECLARE usr_accid INT DEFAULT 0;
	DECLARE usr_name VARCHAR(16) DEFAULT "";
	DECLARE usr_vip INT DEFAULT 0;
	
	DECLARE reward_cursor CURSOR FOR 
		SELECT `id`, `account_id`, `name`, `vip_value` FROM cq_user WHERE vip_value > 0 AND vip_expire > NOW() ORDER BY vip_value DESC, vip_expire DESC;
	
	OPEN reward_cursor;	
	get_vip: LOOP
		FETCH reward_cursor INTO usr_id, usr_accid, usr_name, usr_vip;
		
		IF usr_vip = 1 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000100, usr_accid, 0);
		ELSEIF usr_vip = 2 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000110, usr_accid, 0);
		ELSEIF usr_vip = 3 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000120, usr_accid, 0);
		ELSEIF usr_vip = 4 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000130, usr_accid, 0);
		ELSEIF usr_vip = 5 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000140, usr_accid, 0);
		ELSEIF usr_vip = 6 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000150, usr_accid, 0);
		ELSEIF usr_vip = 7 THEN
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000100, usr_accid, 0);
			INSERT INTO `cq_bonus` (action, id_account, ref_id) VALUES (100000150, usr_accid, 0);
		END IF;
	END LOOP get_vip;
	
	CLOSE reward_cursor;	
END ;;
DELIMITER ;
/*!50003 SET sql_mode              = @saved_sql_mode */ ;
/*!50003 SET character_set_client  = @saved_cs_client */ ;
/*!50003 SET character_set_results = @saved_cs_results */ ;
/*!50003 SET collation_connection  = @saved_col_connection */ ;

/*!50003 DROP PROCEDURE IF EXISTS `GetUsers` */;
/*!50003 SET @saved_cs_client      = @@character_set_client */ ;
/*!50003 SET @saved_cs_results     = @@character_set_results */ ;
/*!50003 SET @saved_col_connection = @@collation_connection */ ;
/*!50003 SET character_set_client  = utf8mb4 */ ;
/*!50003 SET character_set_results = utf8mb4 */ ;
/*!50003 SET collation_connection  = utf8mb4_general_ci */ ;
/*!50003 SET @saved_sql_mode       = @@sql_mode */ ;
/*!50003 SET sql_mode              = 'STRICT_TRANS_TABLES,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION' */ ;
DELIMITER ;;
CREATE DEFINER=`root`@`localhost` PROCEDURE `GetUsers`()
BEGIN
	#Routine body goes here...
	SELECT
		usr.id `Identity`,
		usr.account_id `AccountIdentity`,
		usr.`name` `Name`,
		usr.mateid `MateId`,
		usr.`level` `Level`,
		usr.exp `Experience`,
		usr.profession `Profession`,
		usr.old_prof `OldProfession`,
		usr.first_prof `FirstProfession`,
		usr.metempsychosis `Metempsychosis`,
		usr.strength `Strength`,
		usr.Speed `Agility`,
		usr.health `Vitality`,
		usr.soul `Spirit`,
		usr.additional_point `AdditionalPoint`,
		IFNULL(syn.id, 0) `SyndicateIdentity`,
		IFNULL(attr.rank, 0) `SyndicateRank`,
		usr.donation `NobilityDonation`,
		0 `NobilityRank`,
		IFNULL(sm.number, 0) `SupermanCount`,
		NULL `DeletedAt`,
		usr.money `Money`,
		usr.money_saved `WarehouseMoney`,
		usr.emoney `ConquerPoints`,
		IFNULL(fam.id, 0) `FamilyIdentity`,
		IFNULL(fattr.rank, 0) `FamilyRank`
	FROM
		`cq_user` usr
		LEFT JOIN `cq_synattr` attr ON attr.user_id = usr.id
		LEFT JOIN `cq_syndicate` syn ON syn.id = attr.syn_id
		LEFT JOIN `cq_superman` sm ON sm.user_id = usr.id
		LEFT JOIN `family_attr` fattr ON fattr.user_id = usr.id
		LEFT JOIN `family` fam ON fam.id = fattr.family_id
	WHERE usr.id > 1000000;
END ;;
DELIMITER ;
/*!50003 SET sql_mode              = @saved_sql_mode */ ;
/*!50003 SET character_set_client  = @saved_cs_client */ ;
/*!50003 SET character_set_results = @saved_cs_results */ ;
/*!50003 SET collation_connection  = @saved_col_connection */ ;
/*!50003 DROP PROCEDURE IF EXISTS `QueryFlowerRanking` */;
/*!50003 SET @saved_cs_client      = @@character_set_client */ ;
/*!50003 SET @saved_cs_results     = @@character_set_results */ ;
/*!50003 SET @saved_col_connection = @@collation_connection */ ;
/*!50003 SET character_set_client  = utf8mb4 */ ;
/*!50003 SET character_set_results = utf8mb4 */ ;
/*!50003 SET collation_connection  = utf8mb4_general_ci */ ;
/*!50003 SET @saved_sql_mode       = @@sql_mode */ ;
/*!50003 SET sql_mode              = 'STRICT_TRANS_TABLES,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION' */ ;
DELIMITER ;;
CREATE DEFINER=`root`@`localhost` PROCEDURE `QueryFlowerRanking`(IN rankType INT, IN rankLimit INT, IN rankFrom INT)
BEGIN
	#Routine body goes here...
	IF rankType = 0 THEN
		SELECT p.id,
          p.`name`,
          p.`profession`,
					p.flower_r rose,
					p.flower_w lily,
					p.flower_lily orchid,
					p.flower_tulip tulip,
          @curRank := @curRank + 1 AS rank					
		FROM cq_user p, (SELECT @curRank := 0) r
		WHERE 
			p.id > 1000000
			AND FLOOR(MOD(p.lookface,10000)/1000) = 2			
			AND p.flower_r > 0
		ORDER BY  p.flower_r DESC;
	ELSEIF rankType = 1 THEN 
		SELECT p.id,
          p.`name`,
          p.`profession`,
					p.flower_r rose,
					p.flower_w lily,
					p.flower_lily orchid,
					p.flower_tulip tulip,
          @curRank := @curRank + 1 AS rank
		FROM cq_user p, (SELECT @curRank := 0) r
		WHERE 
			p.id > 1000000
			AND FLOOR(MOD(p.lookface,10000)/1000) = 2			
			AND p.flower_w > 0
		ORDER BY  p.flower_w DESC;
	ELSEIF rankType = 2 THEN 
		SELECT p.id,
          p.`name`,
          p.`profession`,
					p.flower_r rose,
					p.flower_w lily,
					p.flower_lily orchid,
					p.flower_tulip tulip,
          @curRank := @curRank + 1 AS rank
		FROM cq_user p, (SELECT @curRank := 0) r
		WHERE 
			p.id > 1000000
			AND FLOOR(MOD(p.lookface,10000)/1000) = 2			
			AND p.flower_lily > 0
		ORDER BY  p.flower_lily DESC;
	ELSEIF rankType = 3 THEN 
		SELECT p.id,
          p.`name`,
          p.`profession`,
					p.flower_r rose,
					p.flower_w lily,
					p.flower_lily orchid,
					p.flower_tulip tulip,
          @curRank := @curRank + 1 AS rank
		FROM cq_user p, (SELECT @curRank := 0) r
		WHERE 
			p.id > 1000000
			AND FLOOR(MOD(p.lookface,10000)/1000) = 2			
			AND p.flower_tulip > 0
		ORDER BY  p.flower_tulip DESC;
	END IF;
END ;;
DELIMITER ;
/*!50003 SET sql_mode              = @saved_sql_mode */ ;
/*!50003 SET character_set_client  = @saved_cs_client */ ;
/*!50003 SET character_set_results = @saved_cs_results */ ;
/*!50003 SET collation_connection  = @saved_col_connection */ ;
